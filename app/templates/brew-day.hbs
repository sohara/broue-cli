<StatboxGroup>
  <StatboxItem
    @property={{model.recordedEfficiency}}
    @unitOfMesure="%"
    @glyphIcon="glyphicon-signal"
    @propertyDescription="Recorded Efficiency"
    @colorClass="red"
    class="mr-4"
  />
  <StatboxItem
    @property={{apparentAttenuation}}
    @unitOfMesure="%"
    @glyphIcon="glyphicon-sort-by-attributes"
    @propertyDescription="Apparent Attenuation"
    @colorClass="blue"
    class="mr-4"
  />
  <StatboxItem
    @property={{alcoholByVolume}}
    @unitOfMesure="% ABV"
    @faIcon="fa-glass"
    @propertyDescription="Alcohol by Volume"
    @colorClass="yellow"
  />
</StatboxGroup>
{{outlet}}
